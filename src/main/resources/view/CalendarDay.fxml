<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.StackPane?>

<HBox xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" minHeight="30" prefHeight="30"
        minWidth="400" prefWidth="400" VBox.vgrow="ALWAYS" >
    <StackPane minWidth="50" prefWidth="50" HBox.hgrow="NEVER" VBox.vgrow="ALWAYS">
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="darksalmon" width="50" height="30"
                   HBox.hgrow="NEVER" VBox.vgrow="ALWAYS"/>
        <Label fx:id="day" styleClass="cell_small_label" text="\$day"/>
    </StackPane>
    <HBox fx:id="timetable" minWidth="350" prefWidth="350"/>
</HBox>
