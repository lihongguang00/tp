<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/javafx/1">
    <Label fx:id="calendarText" styleClass="label-bright" VBox.vgrow="NEVER" text="\$calendarText"/>
    <VBox fx:id="calendarDayHolder" styleClass="pane-no-border" minWidth="320" prefWidth="320"
          VBox.vgrow="ALWAYS" AnchorPane.topAnchor="30" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10"/>
</AnchorPane>
